        "version": "v1.0.0"
    },
    "host": "127.0.0.1:8080",
    "basePath": "/",
    "paths": {
        "/api/version": {
            "get": {
                "description": "Returns API Version.",
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "App"
                ],
                "summary": "API Version",
                "responses": {
                    "200": {
                        "description": "Current API version",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/system/health": {
            "get": {
                "description": "Returns a \"Working!\" message if the server is healthy.",
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "System"
                ],
                "summary": "Check server health",
                "responses": {
                    "200": {
                        "description": "Working!",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}